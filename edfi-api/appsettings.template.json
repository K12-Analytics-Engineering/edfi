{
    "ApplicationInsights": {
        "InstrumentationKey": "",
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "ConnectionStrings": {
        "EdFi_Ods": "host=/cloudsql/$PROJECT_ID:us-central1:edfi-ods-db;port=5432;username=postgres;password=$DB_PASS;database=EdFi_{0};Application Name=EdFi.Ods.WebApi",
        "EdFi_Security": "host=/cloudsql/$PROJECT_ID:us-central1:edfi-ods-db;port=5432;username=postgres;password=$DB_PASS;database=EdFi_{0};Application Name=EdFi.Ods.WebApi",
        "EdFi_Admin": "host=/cloudsql/$PROJECT_ID:us-central1:edfi-ods-db;port=5432;username=postgres;password=$DB_PASS;database=EdFi_{0};Application Name=EdFi.Ods.WebApi",
        "EdFi_Master": "host=/cloudsql/$PROJECT_ID:us-central1:edfi-ods-db;port=5432;username=postgres;password=$DB_PASS;database=postgres;Application Name=EdFi.Ods.WebApi"
    },
    "BearerTokenTimeoutMinutes": "30",
    "DefaultPageSizeLimit": 500,
    "ApiSettings": {
        "Mode": "InstanceYearSpecific",
        "MinimalTemplateSuffix": "Ods_Minimal_Template",
        "UsePlugins": false,
        "PopulatedTemplateSuffix": "Ods_Populated_Template",
        "PlainTextSecrets": true,
        "MinimalTemplateScript": "PostgreSQLMinimalTemplate",
        "Engine": "PostgreSQL",
        "OdsTokens": [2022,2021,2020],
        "PopulatedTemplateScript": "PostgreSQLPopulatedTemplate",
        "UseReverseProxyHeaders": true,
        "Features": [
            {
                "Name": "OpenApiMetadata",
                "IsEnabled": true
            },
            {
                "Name": "AggregateDependencies",
                "IsEnabled": true
            },
            {
                "Name": "TokenInfo",
                "IsEnabled": true
            },
            {
                "Name": "Extensions",
                "IsEnabled": false
            },
            {
                "Name": "Composites",
                "IsEnabled": true
            },
            {
                "Name": "Profiles",
                "IsEnabled": false
            },
            {
                "Name": "ChangeQueries",
                "IsEnabled": true
            },
            {
                "Name": "IdentityManagement",
                "IsEnabled": false
            },
            {
                "Name": "OwnershipBasedAuthorization",
                "IsEnabled": false
            },
            {
                "Name": "UniqueIdValidation",
                "IsEnabled": false
            }
        ],
        "ExcludedExtensions": []
    },
    "Plugin": {
        "Folder": "./plugins",
        "Scripts": [
            "production"
        ]
    },
    "Caching": {
        "Descriptors": {
            "AbsoluteExpirationSeconds": 1800
        },
        "PersonUniqueIdToUsi": {
            "AbsoluteExpirationSeconds": 0,
            "SlidingExpirationSeconds": 14400
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning"
        }
    }
}